{
  "coin": {
    "name": "mmocoin",
    "shortcut": "MMO",
    "label": "mmocoin",
    "alias": "mmocoin"
  },
  "ports": {
    "backend_rpc": 9902,
    "backend_message_queue": 38359,
    "blockbook_internal": 9332,
    "blockbook_public": 9432
  },
  "ipc": {
    "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
    "rpc_user": "rpc",
    "rpc_pass": "rpc",
    "rpc_timeout": 25,
    "message_queue_binding_template": "tcp://127.0.0.1:{{.Ports.BackendMessageQueue}}"
  },
  "backend": {
    "package_name": "backend-mmocoin",
    "package_revision": "satoshilabs-1",
    "system_user": "mmocoin",
    "version": "2.4.0.0",
    "binary_url": "https://github.com/wkibbler/blockbook/releases/download/v1.3/mmocoin.tar.gz",
    "verification_type": "sha256",
    "verification_source": "fe984ea1e0ef4c4a06fe1414d78b4a37e5b032aa0c4c7d50674c6bc37a90ef53",
    "extract_command": "tar -C backend --strip 1 -xf",
    "exclude_files": [
      "mmocoin-qt"
    ],
    "exec_command_template": "{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/mmocoind -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf={{.Env.BackendInstallPath}}/{{.Coin.Alias}}/{{.Coin.Alias}}.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
    "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/*.log",
    "postinst_script_template": "",
    "service_type": "forking",
    "service_additional_params_template": "",
    "protect_memory": true,
    "mainnet": true,
    "server_config_file": "bitcoin_like.conf",
    "client_config_file": "bitcoin_like_client.conf",
    "additional_params": {
      "deprecatedrpc": "estimatefee"
    }
  },
  "blockbook": {
    "package_name": "blockbook-mmocoin",
    "system_user": "blockbook-mmocoin",
    "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
    "public_binding_template": ":{{.Ports.BlockbookPublic}}",
    "explorer_url": "",
    "additional_params": "",
    "block_chain": {
      "parse": true,
      "mempool_workers": 8,
      "mempool_sub_workers": 2,
      "block_addresses_to_keep": 300,
      "xpub_magic": 76067358,
      "slip44": 175,
      "additional_params": {}
    }
  },
  "meta": {
    "package_maintainer": "Scotty",
    "package_maintainer_email": "scotty.rvn@gmail.com"
  }
}
